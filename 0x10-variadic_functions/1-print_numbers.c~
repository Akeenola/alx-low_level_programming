#include "variadic_functions.h"
#include<stdio.h>
#include<stdarg.h>
void print_numbers(const char *separator, const unsigned int n, ...)
{
  va_list pargs;
  unsigned int k;
  unsigned int i;
  va_start(pargs,n);
  for(i = 0; i<n;i++)
    {
      k = va_arg(pargs,int);
      printf("%d",k);
      printf("%s",separator);
    }
  va_end(pargs);
}
